{
  "receiver": {
    "receivers": [
      { "module": "announced",
        "config": {
          "interval": {
            "statistics": 60,
            "nodeinfo": 500
          }
        }
      },
      { "module": "alfred",
        "alfred_json": "/usr/bin/alfred-json",
        "sockets": [
          "/var/run/alfred-bat0.sock"
        ],
        "config": {
          "interval": {
            "statistics": 60,
            "nodeinfo": 500
          }
        }
      },
      { "module": "aliases",
        "config": {
          "file": "./aliases.json"
        },
        "overlay": true
      }
    ],
    "ifaces": [
      "bat0"
    ],
    "storage": {
      "file": "./raw.json"
    },
    "purge": {
      "maxAge": 14
    }
  },
  "provider": {
    "offlineTime": 900
  },
  "webserver": {
    "port": 4000
  }
}
