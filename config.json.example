{
  "core": { },
  "receiver": {
    "receivers": [
      { "module": "announced",
        "config": {
          "target": {
            "ip": "ff02::1",
            "port": 1001
          },
          "port": 45123,
          "interval": {
            "statistics": 60,
            "nodeinfo": 500
          }
        },
        "overlay": false
      },
      { "module": "aliases",
        "config": {
          "file": "./aliases.json"
        },
        "overlay": true
      }
