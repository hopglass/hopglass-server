    "purge": {
      "maxAge": 14,
      "interval": 86400
    }
  },
  "provider": {
    "offlineTime": 900,
    "metricsOfflineTime": 60,
    "ffapiPath": "./ffapi/"
  },
  "webserver": {
    "ip": "::",
    "port": 4000
  }
